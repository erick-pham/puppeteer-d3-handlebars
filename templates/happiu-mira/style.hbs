  }

  .happiu-score {
    width: 400px;
    border: 1px solid #b6b6b640;
    padding: 30px 20px;
    background: #ffffff;
  }

  .border-text {
    width: 50%;
    margin-right: 50px;
  }

  .border-chart {
    width: 50%;
    display: flex;
    justify-content: flex-end;
  }

  .percent {
    font-family: PTSans-Bold;
    font-size:
      40px;
    color: #00247a;
  }

  .total-percent {
    font-size: 20px;
    color: #a2a5d4;
  }

  .status {
    color: white;
    height: 22px;
    display: inline;
    min-width: 40px;
    border-radius: 100px;
    justify-content: center;
    padding: 2px 0;
  }

  .status-medium {
    color: #d68000;
    border-radius: 100px;
    margin-left: 20px;
    padding: 0px 10px;
  }

  .status-high {
    color: #00a53b;
    border-radius: 100px;
    margin-left: 20px;
    padding: 0px 10px;
  }

  .text-gap {
    font-size: 20px;
    color:
      #000000;
    font-family: PTSans-Bold;
    padding: 5px 0px;
  }

  .status-gap-potential {
    font-size: 18px;
    color: #f07662;
  }

  .status-gap-better {
    color: #fed586;
    font-size: 18px;
  }

  .status-great-improvement {
    color: #00a53b;
    font-size:
      18px;
  }

  /* chart */
  .gauge {
    position: relative;
    width: 140px;
    max-width:
      140px;
    font-size: 32px;
    color: #004033;
  }

  .gauge-body {
    width: 100%;
    height:
      0;
    padding-bottom: 50%;
    background: #e9e7eb;
    position: relative;
    border-top-left-radius: 100% 200%;
    border-top-right-radius: 100% 200%;
    overflow: hidden;
  }

  .gauge-fill {
    position: absolute;
    top: 100%;
    left: 0;
    width: inherit;
    height: 100%;
    transform-origin: center top;
    transition:
      transform 0.2s ease-in-out;
  }

  .gauge-cover {
    width: 88%;
    height: 180%;
    background: white;
    border-radius: 50%;
    position: absolute;
    top: 10%;
    left:
      50%;
    transform: translateX(-50%);
  }

  .circle-dot {
    position: absolute;
    width:
      100px;
    bottom: 6px;
    position: absolute;
    left: 50%;
    margin-left: -50px;
  }

  .needle {
    position: absolute;
    z-index: 100;
    left: 50%;
    margin-left: -48px;
    bottom: -45px;
    width: 100px;
    height: auto;
    transition: "transform 1s";
    transform-origin: center center;
  }

  .gauge-score {
    position: absolute;
    top:
      80%;
    left: 60%;
    font-size: 12px;
    font-weight: bold;
    width: 65px;
    margin: -15px -30px;
    transform: rotate(-0.15turn) translate(78px) rotate(0.15turn);
  }

  /*
  end-chart */
  .icon-reply-outlined {
    width: 20px;
    height: 20px;
  }

  .text-reply-outlined {
    font-size: 20px;
    font-family: PTSans-Bold;
  }

  /*
  chart-round */
  .chart {
    position: relative;
    width: 100px;
    height: 100px;
    max-width: 100px;
    font-size: 32px;
    color: #000000;
  }

  .chart-body {
    width:
      100%;
    height: 0;
    padding-bottom: 100%;
    background: #e9e7eb;
    position:
      relative;
    border-radius: 50%;
    overflow: hidden;
  }

  .chart-cover {
    width: 81%;
    height: 81%;
    text-align: center;
    background: white;
    top: 50%;
    left: 50%;
    /*
  padding-bottom: 80%; */
    position: absolute;
    border-radius: 50%;
    overflow:
      hidden;
    transform: translate(-50%, -50%);
  }

  .chart-cover .text-percent {
    font-size: 24px;
    margin-top: 10px;
  }

  .chart-cover .text-percent span {
    font-size: 13px;
    color: #cccccc;
  }

  .chart-cover .text-gap-covered {
    font-size:
      16px;
    color: #000000;
    line-height: initial;
  }

  .progress-ring__circle {
    transition: 0.35s stroke-dashoffset;
    transform: rotate(-90deg);
    transform-origin: 48% 48%;
    position: absolute;
  }

  /* end chart round */
  .pl-20 {
    padding-left: 20px;
    padding-right: 1px;
  }

  .border-recommended {
